<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<window xmlns="http://schemas.haulmont.com/cuba/screen/window.xsd"
        caption="DB Schema"
        focusComponent="form">
    <data>
        <instance id="modelSchemaDc"
                  class="ch.so.agi.simi.entity.data.DataTheme" view="dataTheme-edit">
            <loader/>
        </instance>
    </data>
    <dialogMode height="AUTO"
                width="AUTO"
                modal="true"
                forceDialog="true"/>
    <layout expand="editActions" spacing="true">
        <form id="form" dataContainer="modelSchemaDc">
            <column width="350px">
                <textField id="schemaNameField" property="schemaName"/>
                <textField id="modelNameField" property="modelName"/>
                <pickerField id="postgresDBField" property="postgresDB">
                    <actions>
                        <action id="lookup" type="picker_lookup"/>
                        <action id="clear" type="picker_clear"/>
                    </actions>
                </pickerField>
            </column>
        </form>
        <hbox id="editActions" spacing="true">
            <button action="windowCommitAndClose"/>
            <button action="windowClose"/>
        </hbox>
    </layout>
</window>
